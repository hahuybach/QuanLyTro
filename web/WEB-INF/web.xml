<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>LoginController</servlet-name>
        <servlet-class>controller.LoginController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>HomeController</servlet-name>
        <servlet-class>controller.HomeController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SearchTenantController</servlet-name>
        <servlet-class>controller.tenant.SearchTenantController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>InsertTenantController</servlet-name>
        <servlet-class>controller.tenant.InsertTenantController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdateTenantController</servlet-name>
        <servlet-class>controller.tenant.UpdateTenantController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteTenantController</servlet-name>
        <servlet-class>controller.tenant.DeleteTenantController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogOutController</servlet-name>
        <servlet-class>controller.LogOutController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SearchRoomController</servlet-name>
        <servlet-class>controller.room.SearchRoomController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>InsertRoomController</servlet-name>
        <servlet-class>controller.room.InsertRoomController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteRoomController</servlet-name>
        <servlet-class>controller.room.DeleteRoomController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdateRoomController</servlet-name>
        <servlet-class>controller.room.UpdateRoomController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DetailRoomController</servlet-name>
        <servlet-class>controller.room.DetailRoomController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginController</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>HomeController</servlet-name>
        <url-pattern>/home</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SearchTenantController</servlet-name>
        <url-pattern>/tenant/search</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>InsertTenantController</servlet-name>
        <url-pattern>/tenant/insert</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpdateTenantController</servlet-name>
        <url-pattern>/tenant/update</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteTenantController</servlet-name>
        <url-pattern>/tenant/delete</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogOutController</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SearchRoomController</servlet-name>
        <url-pattern>/room/search</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>InsertRoomController</servlet-name>
        <url-pattern>/room/insert</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteRoomController</servlet-name>
        <url-pattern>/room/delete</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpdateRoomController</servlet-name>
        <url-pattern>/room/update</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DetailRoomController</servlet-name>
        <url-pattern>/room/detail</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
